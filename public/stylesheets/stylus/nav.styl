@import "mix_default.styl"
  /*------导航栏-------*/
.nav
  width 100%
  height  50px
  position  fixed
  top 0
  left  300px
  background  white
  z-index 9999

.nav_Menu
  width auto
  max-width 400px
  float left
  height  50px
  left  0
  top 0

.nav_Menu>li
  width 80px
  height  50px
  font-size 14px
  float left
  text-align  center
  line-height 51px
.nav_Menu>li>a
  article-word_color()

  /*-------nav下拉菜单--------*/
.nav-setting
  position  fixed
  right 30px
  top 10px
